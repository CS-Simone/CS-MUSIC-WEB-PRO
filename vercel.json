{
  "version": 2,
  "builds": [
    { "src": "api/albums.js", "use": "@vercel/node" },
    { "src": "api/audio.js", "use": "@vercel/node" },
    { "src": "api/db.js", "use": "@vercel/node" },
    { "src": "api/login.js", "use": "@vercel/node" },
    { "src": "api/upload.js", "use": "@vercel/node" },
    { "src": "public/*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/login", "dest": "/public/login.html" },
    { "src": "/admin", "dest": "/public/admin.html" },
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/(.*)", "dest": "/public/$1" },
    { "src": "/api/albums", "dest": "/api/albums.js" },
    { "src": "/public/style.css", "dest": "/public/styles.css" },
  ]
}
